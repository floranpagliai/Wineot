{{ form_start(form) }}
        {% if errors  is defined %}
            {% for error in errors %}
                <div class="alert alert-danger"><i class="fa fa-warning"></i> {{ error.message|trans }}</div>
            {% endfor %}
        {% endif %}
<fieldset>
    <div class="form-group">
        <label class="sr-only" for="country">Country</label>
        {{ form_widget(form.name, { 'attr': { 'placeholder': 'country'|trans } } ) }}
    </div>
    <div class="form-group">
        <label class="sr-only" for="code">Code</label>
        {{ form_widget(form.code, { 'attr': { 'placeholder': 'code'|trans } } ) }}
    </div>
    {% include 'WineotBackEndCRUDBundle:Region:form.html.twig' %}
    <br>
    <input class="btn btn-lg  btn-block" type="submit" id="_submit" name="submit_country"
           value="{{ 'submit'|trans }}"/>
</fieldset>
<br>
{#{{ form_end(form) }}#}
{{ form_widget(form._token) }}